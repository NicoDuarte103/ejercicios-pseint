Algoritmo sin_titulo
	definir distancia como entero;
	definir velocidad como entero;
	definir tiempo como entero;
	
	Escribir "escriba la velocidad del automovil en metros";
	leer velocidad;
	Escribir "escriba el tiempo que tardo en recorrer la distancia en segundos";
	leer tiempo;
	
	distancia = velocidad * tiempo;
	
	Escribir "la distancia recorrida es",distancia "metros";
	
	
FinAlgoritmo
